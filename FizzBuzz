counter = 1
while counter < 101 
    if (counter % 3 == 0) && (counter % 5 == 0)
        puts "FizzBuzz"
        counter += 1
    elsif (counter % 3 == 0)
        puts "Fizz"
        counter += 1
    elsif (counter % 5 == 0)
        puts "Buzz"
        counter += 1
    else
        puts counter
        counter += 1
    end
end
